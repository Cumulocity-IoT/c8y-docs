  <!-- -->
  <nav class="navigator" >
    <div class="sg-title text-center">
      <a href="/guides" class="no-ajaxy">
        <i class="c8y-icon c8y-icon-c8y" style="font-size: 120px;"></i>
      </a>
      <div class="dropdown text-left">
        <a href="#" class="dropdown-toggle no-ajaxy" data-toggle="dropdown">
          <span class="current text-truncate" style="text-transform: none;">Guides</span>
        </a>
        <ul class="dropdown-menu">
          <% for section in @sections:%>
            <% $first = '' %>
            <% for page in @getCollection("html").findAllLive({relativeOutDirPath: section.folder }, [ order: 1, title: 1 ] ).toJSON():%>
              <% if page.id is @document.id then $first = 'active' %>
            <% end %>
            <li class="<%= $first %> lang-<%= section.lang %>">
              <a href="/<%= section.folder %>" class="no-ajaxy">
                <span class="<%= section.icon %>"></span> <%= section.title %>
              </a>
            </li>
          <% end %>
        </ul>
      </div>
    </div>

    
 
    <ul id="mainnav" class="navigatorContent children">
        <% for section in @sections:%>
          <% $first = '' %>
          <% for page in @getCollection("html").findAllLive({relativeOutDirPath: section.folder }, [ order: 1, title: 1 ] ).toJSON():%>
            <% if page.id is @document.id then $first = 'current' %>
          <% end %>
        <% if $first is 'current' : %>
          <% for page in @getCollection("html").findAllLive({relativeOutDirPath: section.folder }, [ order: 1, title: 1 ] ).toJSON():%>
            <li class="slot <%= if page.id is @document.id then 'current' %>">
              <a href="<%= page.url%>" title="<%= page.title %>" class="link "><span><%= page.title || 'no title' %></span></a>
                <% if page.collection : %>

                  <a href="javascript:void(0);" data-toggle="collapse" data-target="#sub-<%=page.basename %>"  class="<%= 'collapsed' if @document.url isnt page.url %>" ><i class="fa fa-chevron-right " ></i></a>
     
                <ul id="sub-<%=page.basename %>" class="nav navbar-nav subchildren collapse <%= 'in' if @document.url is page.url %>" >
                  <% for subsubdocument in @getCollection("html").findAllLive({relativeOutDirPath: page.collection }, [ order: 1, title: 1 ] ).toJSON(): %>
                 <li>
                    <a href="#<%= subsubdocument.basename %>" data-target="#<%= subsubdocument.basename %>" title="<%= subsubdocument.title %>" data-href="<%= page.url %>#<%= subsubdocument.basename %>">
                      <span><%= subsubdocument.title %></span>
                    </a>
                  </li>
                  <% end %>
                  </ul>
                <% end %>
            </li>
          <% end %>
        <% end %>
      <% end %>
    </ul>

    <div class="powered-by">
      <p><small>&copy; Cumulocity <%- (new Date()).getFullYear() %></small></p>
    </div>
  </nav>

  <div class="cover"></div>
    <!-- Content -->
  <div class="main-content">
    
    <article class="content">
        <%- @partial('toc.html.eco', {tocItems: @document.tableOfContents}) %>
        <%- @content %>
    </article>
  </div>




